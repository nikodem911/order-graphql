type Order {
    id: ID!
    items: [OrderItem!]!
    customer: Customer!
}

type OrderItem {
    id: ID!
    product: Product!
    quantity: Int!
}

type Product {
    id: ID!
    name: String!
    price: Float!
}

type Customer {
    id: ID!
    firstName: String!
    lastName: String!
}

type Query {
    orders: [Order!]!
    products: [Product!]!
    customers: [Customer!]!
}